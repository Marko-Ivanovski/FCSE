#include <iostream>

using namespace std;

int sum_elements(int* array, int n) {
    if (n == 0) {
        return array[n];
    } else {
        return array[n] + sum_elements(array, n - 1);
    }
}

int main() {
    int i, n, a[100];

    cin>>n;

    for (i = 0; i < n; i++){
        cin>>a[i];
    }

    cout << "The sum of all of the array elements is: " << sum_elements(a, n - 1) << endl;

    return 0;
}
