#include <iostream>

using namespace std;

int GCD(int m, int n) {
    if (!n) {
        return m;
    }
    return GCD(n, m % n);
}

int main() {
    int i, n, a[100];
    
    cout << "Size of the array: ";
    cin >> n;

    cout << "Elements of the array: \n";
    for (i = 0; i < n; ++i) {
        cin >> a[i];
    }

    int gcd = GCD(a[0], a[1]);
    for (i = 2; i < n; ++i) {
        gcd = GCD(gcd, a[i]);
    }

    cout << "GCD of the elements in this array is " << gcd;

    return 0;
}
