#include <iostream>

using namespace std;

void inputArray(int* array, int n){
    for (int i = 0; i < n; i++) {
        cin>>array[i];
    }
}

void printArray(int* array, int n){
    for (int i = 0; i < n; ++i) {
        cout<<array[i]<<" ";
    }
}

void bubbleSort(int* array, int n){
    for (int i = 0; i < n-1; i++) {
        for (int j = 0; j < n-i-1; j++) {
            if(array[j]>array[j+1]){
                swap(array[j],array[j+1]);
            }
        }
    }
}

int main(){
    int n;
    int array[100];
    cin>>n;

    inputArray(array,n);

    printArray(array,n);
    cout<<endl;

    bubbleSort(array,n);

    printArray(array,n);
    return 0;
}
