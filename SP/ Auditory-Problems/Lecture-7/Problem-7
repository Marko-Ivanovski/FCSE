#include <iostream>

using namespace std;

int main() {
    int n, deleted = 0;
    int array[100];
    cout<<"Enter size of array: ";
    cin>>n;
    cout<<"Enter array elements: ";
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    // Loop to remove duplicate elements from the array
    for(int i=0;i<n-deleted;i++){
        for(int j=i+1;j<n-deleted;j++){
            // Check if two elements are equal
            if(array[i]==array[j]){
                // If equal, shift elements to the left to remove the duplicate
                for(int k=j;k<n-1-deleted;k++){
                    array[k] = array[k+1];
                }
                // Increment the count of deleted elements and adjust the loop counter
                deleted++;
                --j;
            }
        }
    }
    // Update the size of the array after removing duplicates
    n-=deleted;

    // Output the array without duplicate elements
    for(int i=0;i<n;i++){
        cout<<array[i]<<" ";
    }
    return 0;
}
