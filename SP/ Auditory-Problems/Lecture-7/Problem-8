#include <iostream>

using namespace std;

int main() {
    int n, m, sumCols = 0, sumRows = 0;
    int matrix[100][100];

    cout<<"Enter the size of the matrix: ";
    cin>>n>>m;
    cout<<"Enter the elements of the array: ";
    for(int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin>>matrix[i][j];
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; ++j) {
            if ((j+1)%2){
                sumCols+=matrix[i][j];
            }
            if (!((i+1)%2)){
                sumRows+=matrix[i][j];
            }
        }
    }
    cout<<sumCols-sumRows;
    
    return 0;
}
