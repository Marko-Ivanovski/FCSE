#include <iostream>

using namespace std;

int main(){
  int n, element;
  int array[100];
  int increasing = 1, decreasing = 1;

  cin>>n;

  for(int i = 0; i < n; i++) {
      cin>>array[i];
  }
  for (int i = 0; i < n - 1; i++) {
        if (a[i] >= a[i + 1]) {
            increasing = 0;
            break;
        }
  }
  for (int i = 0; i < n - 1; i++) {
        if (a[i] <= a[i + 1]) {
            decreasing = 0;
            break;
        }
  }

  if (!decreasing && !increasing) {
        cout << "The array is neither strictly increasing nor strictly decreasing" << endl;
    } else if (decreasing) {
        cout << "The array is strictly decreasing" << endl;
    } else if (increasing) {
        cout << "The array is strictly increasing" << endl;
    }

    return 0;
}
