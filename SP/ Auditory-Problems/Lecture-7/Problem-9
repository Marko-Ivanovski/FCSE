#include <iostream>

using namespace std;

int main() {
    int n, min, max;
    int matrix[100][100];
    cin>>n;

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            cin>>matrix[i][j];
        }
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            if (i==0 && j==0){
                min=max=matrix[i][j];
            } else if (matrix[i][j]>max) {
                max=matrix[i][j];
            } else if (matrix[i][j]<min) {
                min=matrix[i][j];
            }
        }
    }

    for (int i = 0; i < n; i++) {
        matrix[i][i] = max - min;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }
    
    return 0;
}
