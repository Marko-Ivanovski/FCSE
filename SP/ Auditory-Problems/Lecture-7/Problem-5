#include <iostream>

using namespace std;

int main() {
    int n, temp;
    int array[100];
    cout<<"Enter the size of the array: ";
    cin>>n;
    cout<<"Enter the array elements: ";
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    temp = array[n-1];

    for(int i=n-1;i>0;i--){
        array[i] = array[i-1];
    }
    array[0] = temp;

    for(int i=0;i<n;i++){
        cout<<array[i]<<" ";
    }
    return 0;
}
