    #include <iostream>
    using namespace std;
    
    int sum(int n) {
        return n % 100 + n / 100;
    }
    
    int main() {
        int i;
        int count = 0;
        for(i = 1000; i <= 9999; ++i) {
            if(i % sum(i) == 0) {
                cout << i << endl;
                ++count;
            }
        }
        cout << "Total:  " << count;
        return 0;
    }
