    #include <iostream>
    using namespace std;
    
    int prime(int n) {
        int i;
        for(i = 2; i * i <= n; ++i) {
            if(n % i == 0) {
                return 0;
            }
        }
        return 1;
    }
    
    int main() {
        int i, count = 0;
        for(i = 1; i < 998; ++i) {
            if(prime(i) && prime(i + 2)) {
                cout << "(" << i << ", " << i+2 <<")" << endl;
                ++count;
            }
        }
        cout << "Total: " << count << endl;
        return 0;
    }
