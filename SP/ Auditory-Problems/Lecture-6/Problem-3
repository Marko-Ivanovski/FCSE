    #include <iostream>
    using namespace std;
    
    int prime(int n) {
        int i;
        for(i = 2; i * i <= n; ++i) {
            if(n % i == 0) {
                return 0;
            }
        }
        return 1;
    }
   
    int firstLargerPrime(int n) {
        do{
            ++n;
        }while(!prime(n));
        return n;
    }
    
    int main() {
        int n;
        cin >> n;
        int largerPrime = firstLargerPrime(n);
        cout << largerPrime << " " << largerPrime - n;
        return 0;
    }
